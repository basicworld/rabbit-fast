<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<!--避免生成重复代码的插件 -->
		<plugin type="com.rabbit.common.util.sql.OverIsMergeablePlugin" />

		<!--是否在代码中显示注释 -->
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!--数据库链接地址账号密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost/rabbit2" userId="root"
			password="123456">
			<!--设置为 true 可以获取 tables 信息, 解决生成文件缺少 xxxByPrimaryKey 的问题 -->
			<property name="useInformationSchema" value="true" />
		</jdbcConnection>
		<!--不知道做什么用的。。。反正贴上来了~ -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!--生成pojo类存放位置 -->
		<javaModelGenerator targetPackage="com.rabbit.system.domain" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!--生成xml映射文件存放位置 -->
		<sqlMapGenerator targetPackage="system" targetProject="src/main/resources/mybatis">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!--生成mapper类存放位置 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.rabbit.system.mapper" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!--生成对应表及类名 -->

		<table tableName="sys_account" domainObjectName="SysAccount" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_user" domainObjectName="SysUser" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_role" domainObjectName="SysRole" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_menu" domainObjectName="SysMenu" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_dept" domainObjectName="SysDept" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_dept_user" domainObjectName="SysDeptUser" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_dept_role" domainObjectName="SysDeptRole" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_user_role" domainObjectName="SysUserRole" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
		<table tableName="sys_role_menu" domainObjectName="SysRoleMenu" enableCountByExample="false" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="false">
			<property name="my.isgen.usekeys" value="true" />
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>

	</context>
</generatorConfiguration>